{
    "name": "feall-desktop-vue",
    "version": "0.0.1",
    "description": "@fe/desktop-vue",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "main": "./build/index.js",
    "scripts": {
        "start:build": "npm run tsc && node build",
        "start": "node run",
        "test": "jest --forceExit --detectOpenHandles --colors",
        "clear": "rimraf build",
        "tsc": "npm run clear && tsc",
        "tsc:w": "npm run tsc -- -w",
        "p": "npm run tsc && npm publish",
        "lint": "tslint --fix '{src,__tests__}/**/*.ts'",
        "precommit": "npm run lint"
    },
    "dependencies": {
        "globby": "^11.0.1",
        "multiple-package-build": "^1.0.6",
        "multiple-package-publish": "^1.0.8",
        "tslib": "^2.0.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.13",
        "@types/node": "^12.12.53",
        "husky": "^4.2.3",
        "jest": "^26.4.2",
        "rimraf": "^2.7.1",
        "ts-jest": "^26.3.0",
        "ts-node": "^8.10.2",
        "tslint": "^6.1.2",
        "typescript": "^3.9.7"
    },
    "keywords": [
        "typescript",
        "node-frontend-architecture"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    },
    "author": "hubiqing",
    "repository": "https://github.com/githbq/node-frontend-architecture.git",
    "engines": {
        "node": ">=10"
    },
    "license": "MIT"
}